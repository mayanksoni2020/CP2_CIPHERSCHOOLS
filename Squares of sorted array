class Solution {
public:
    vector<int> sortedSquares(vector<int>& A) {
        vector<int>::iterator it;
        vector<int> B;
        for(it=A.begin();it!=A.end();it++)
        {
               B.push_back((*it)*(*it));
        }
        sort(B.begin(),B.end());
        
        return B;
    }
};
