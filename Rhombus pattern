#include <iostream>
#include <string>
using namespace std;
int main()
{
    int n=4;
    int size=n+(n-1);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<size;j++)
        {
            if((i+j<3) ||(i+j>=7))
            cout<<" ";
            else
            cout<<"*";
        }
        cout<<"\n";
    }
}
